    <calculatedAttribute id="source_url" hidden="false" order="29" semanticType="empty" attributeHierarchyActive="false" displayAttribute="false">
      <descriptions defaultDescription="source_url"/>
      <keyCalculation datatype="NVARCHAR" length="200">
        <formula>leftstr(rightstr(&quot;source&quot;,strlen(&quot;source&quot;)-instr(&quot;source&quot;,'&quot;')),instr(rightstr(&quot;source&quot;,strlen(&quot;source&quot;)-instr(&quot;source&quot;,'&quot;')),'&quot;')-1)</formula>
      </keyCalculation>
    </calculatedAttribute>
    <calculatedAttribute id="text_head" hidden="false" order="30" semanticType="empty" attributeHierarchyActive="false" displayAttribute="false">
      <descriptions defaultDescription="text_head"/>
      <keyCalculation datatype="NVARCHAR" length="200">
        <formula>if(&quot;TA_OFFSET&quot;=0,'',leftstr(&quot;text&quot;,(int(&quot;TA_OFFSET&quot;)+1)-1))</formula>
      </keyCalculation>
    </calculatedAttribute>
    <calculatedAttribute id="text_tail" hidden="false" order="31" semanticType="empty" attributeHierarchyActive="false" displayAttribute="false">
      <descriptions defaultDescription="text_tail"/>
      <keyCalculation datatype="NVARCHAR" length="200">
        <formula>if((&quot;TA_OFFSET&quot;+1)+strlen(&quot;token&quot;)=strlen(&quot;text&quot;),'',rightstr(&quot;text&quot;,strlen(&quot;text&quot;)-(int(&quot;TA_OFFSET&quot;)+1)-strlen(&quot;token&quot;)+1))</formula>
      </keyCalculation>
    </calculatedAttribute>
