        <formula>if(&quot;sentiment&quot; = 'StrongPositiveSentiment', 'Strong Positive', if(&quot;sentiment&quot; = 'WeakPositiveSentiment', 'Weak Positive', if(&quot;sentiment&quot; = 'NeutralSentiment', 'Neutral', if(&quot;sentiment&quot; = 'WeakNegativeSentiment', 'Weak Negative', 'Strong Negative'))))</formula>
